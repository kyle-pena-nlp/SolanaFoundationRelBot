{
    "compilerOptions": {
      "target": "esnext",
      "module": "esnext",
      "lib": ["esnext", "webworker"],
      "outDir": "dist",
      "strict": true,
      "alwaysStrict": true,
      "moduleResolution": "node",
      "esModuleInterop": true,
      "sourceMap": true,
      "preserveConstEnums": true,
      "importHelpers": true,
      "types": [
        "@cloudflare/workers-types/2023-07-01", 
        "node", 
        "@types/jest"
      ]
    },
    "include": [
      
      "./**/*.ts",
      "./*.ts",
      "./node_modules/@cloudflare/workers-types/*.ts"   
   ],
   "exclude": [
   ]
}